{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Next.js Debug",
        "program": "${workspaceFolder}/node_modules/.bin/next",
        "args": ["dev"],
        "autoAttachChildProcesses": true,
        "protocol": "inspector",
        "restart": true,
        "runtimeExecutable": null,
        "console": "integratedTerminal"
      }
    ]
  }
  